<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Button?>

<?import javafx.scene.image.ImageView?>
<?import javafx.scene.image.Image?>

<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="ui.MainAppController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label>
    <graphic>
        <ImageView fitHeight="100.0" fitWidth="100.0" >
            <Image url="@./img/germ.png" />
        </ImageView>
    </graphic>
    </Label>
    <Button text="Операции над двумя функциями" onAction="#onOperationsButtonClick"/>
    <Button text="Дифференцирование функции" onAction="#onDiffButtonClick"/>
    <Button text="Интеграл" onAction="#onIntegralButtonClick"/>
    <Button text="Настройки" onAction="#onSettingsButtonClick"/>
</VBox>